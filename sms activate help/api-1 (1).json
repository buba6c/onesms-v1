          },
          "ru": {
            "description": "Название на русском",
            "type": "string"
          },
          "eng": {
            "description": "Название на английском",
            "type": "string"
          },
          "chinese": {
            "description": "Название на китайском",
            "type": "string"
          },
          "pt": {
            "description": "Название на португальском",
            "type": "string"
          },
          "es": {
            "description": "Название на испанском",
            "type": "string"
          },
          "simCount": {
            "description": "Количество SIM",
            "type": "string"
          },
          "code": {
            "description": "Country code",
            "type": "integer"
          },
          "show": {
            "description": "Отображение страны",
            "type": "boolean"
          },
          "sorting": {
            "description": "сортировка, больше число - выше в списке",
            "type": "integer"
          },
          "continent": {
            "description": "0 - Европа, 1 - Азия, 2 - Африка, 3 - Северная Америка, 4 - Южная Америка, 5 - Австралия",
            "type": "integer"
          },
          "promo": {
            "description": "0 - без промо, 1 - пламя, 2 - new",
            "type": "integer"
          },
          "addSms": {
            "description": "Возможность приема дополнительной смс",
            "type": "integer"
          },
          "rent": {
            "description": "0-нельзя под аренду, 1-можно под аренду",
            "type": "integer"
          },
          "otNote": {
            "description": "Уведомление о пустом ot",
            "type": "integer"
          },
          "multiService": {
            "description": "Множественные услуги",
            "type": "integer"
          },
          "addedToHub": {
            "description": "Добавление в хаб",
            "type": "boolean"
          },
          "countNullCheck": {
            "description": "Используется для проверки наличия номеров",
            "type": "integer"
          },
          "countNullCheckRent": {
            "description": "Для проверки и отображения аренды",
            "type": "integer"
          }
        },
        "type": "object"
      },
      "EmailActivationCreateDTO": {
        "required": [
          "userId",
          "kopeechkaId",
          "site",
          "email",
          "status",
          "cost",
          "nativeCost",
          "vendor",
          "currency",
          "domain",
          "userRank",
          "source"
        ],
        "properties": {
          "userId": {
            "type": "integer",
            "example": 123
          },
          "kopeechkaId": {
            "type": "integer",
            "example": 456
          },
          "site": {
            "type": "string",
            "example": "example.com"
          },
          "email": {
            "type": "string",
            "format": "email",
            "example": "test@example.com"
          },
          "status": {
            "type": "integer",
            "example": 1
          },
          "cost": {
            "type": "number",
            "format": "float",
            "example": 0.25
          },
          "nativeCost": {
            "type": "number",
            "format": "float",
